// a - stream ssim
// b - required minimum ssim
// c - frame offset
// d - ssim of frame at offset c

def main(private field[10] a, field b, field c, field d) -> (field):
	field match = 0
	field valid = 0
	for field i in 0..10 do
	  match = match + if b < a[i] then 1 else 0 fi
	endfor
	valid = if a[c] == d then 1 else 0 fi
	return if match >= 7 && valid == 1 then 1 else 0 fi